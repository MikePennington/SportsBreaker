<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.breaker.team">
	<class name="TeamBean" table="teams">

		<id name="teamId" column="id">
			<generator class="native" />
		</id>
		<property name="teamName" column="name" />
		<property name="categoryId" column="category_id" />
		<property name="location" column="location" />
		<property name="conference" column="conference" />
		<property name="subconference" column="subconference" />
		<property name="college" column="is_college" />
		<property name="collegeId" column="college_id" />

	</class>
</hibernate-mapping>